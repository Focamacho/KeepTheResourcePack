{
  "schemaVersion": 1,
  "id": "keeptheresourcepack",
  "version": "${version}",
  "name": "KeepTheResourcePack",
  "description": "Keep the Resource Pack loaded even after leaving the server.",
  "authors": [
    "focamacho"
  ],
  "contact": {
    "repo": "https://github.com/focamacho/KeepTheResourcePack"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.focamacho.keeptheresourcepack.client.KeepTheResourcePackClient"
    ],
    "main": [
      "com.focamacho.keeptheresourcepack.KeepTheResourcePack"
    ]
  },
  "mixins": [
    "keeptheresourcepack.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": ">=1.20.3"
  },
  "accessWidener" : "keeptheresourcepack.accesswidener"
}
